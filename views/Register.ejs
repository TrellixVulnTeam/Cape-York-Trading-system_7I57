<form action="/register" method="POST">
    <label for="firstName">First Name</label>
    <input type="text" id="firstName" name="fName" required><br>

    <label for="lastName">Last Name</label>
    <input type="text" id="lastName" name="lName" required><br>

    <label for="email">Email </label>
    <input type="text" id="email" name="email" required><br>

    <label for="password">Password </label>
    <input type="text" id="password" name="password" required><br>

    <label for="confirmPassword">Confirm Password</label>
    <input type="text" id="confirmPassword" name="conPassword" required><br>

    <label for="dropdownMenuButton1">Community</label>
    
    
    <input type="checkbox" id="agree" required>
    <label for="Communities">I agree with the terms and conditions of this website</label>
    <br>
    <select name="Communities" id="Communities">

    </select>

    <button type="submit">Submit</button><br>

    <a href="/">Already have an account</a>


</form>

